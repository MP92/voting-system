<div class="modal fade" id="profile">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h2 class="modal-title">Profile</h2>
            </div>
            <div class="modal-body">
                <div class="panel panel-info" id="profileInfo">
                    <div class="panel-heading">
                    </div>
                    <div class="panel-body">
                        <table class="table table-user-information">
                            <tbody>
                            <tr>
                                <th>ID</th>
                                <td class="profile-id"></td>
                            </tr>
                            <tr>
                                <th>Name</th>
                                <td class="profile-name"></td>
                            </tr>
                            <tr>
                                <th>Surname</th>
                                <td class="profile-surname"></td>
                            </tr>
                            <tr>
                                <th>Registered</th>
                                <td class="profile-registered"></td>
                            </tr>
                            <tr>
                                <th>Enabled</th>
                                <td class="profile-enabled"></td>
                            </tr>
                            <tr>
                                <th>Roles</th>
                                <td class="profile-roles"></td>
                            </tr>
                            <tr>
                                <th>Last voted</th>
                                <td class="profile-lastVoted"></td>
                            </tr>
                            <tr>
                                <th>Last chosen restaurant</th>
                                <td class="profile-restaurant"></td>
                            </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>